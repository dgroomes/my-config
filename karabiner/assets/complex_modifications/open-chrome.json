{
  "title": "Open Apps with tab modifier",
  "rules": [
    {
      "description": "Open Chrome with 'tab + c'",
      "manipulators": [
        {
          "conditions": [
            {
              "type": "variable_if",
              "name": "tab_modifier",
              "value": 1
            }
          ],
          "from": {
            "key_code": "c"
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}